{
  "name": "Extension Showcase",
  "description": "Sample extension to showcase the SAP FSM Extensions Update.",
  "version": "78.0.0",
  "icon": "https://raw.githubusercontent.com/scamastral/sample-fsm-extensions/master/parameter-showcase-extension/parameter-icon.png",
  "useShellSDK": false,
  "outletPositions": ["WFM_ACTIVITY_SIDEBAR", "SHELL_HOME_SCREEN", "PP_DASHBOARD_1", "PP_TECHNICIAN_DETAILS_1", "MDM_EQUIPMENT_DETAIL_PAGE_BOTTOM"],
  "optimalHeight": "320px",
  "shellNavigation": null,
  "provider": "SAP",
  "extensionAttributes": [
    "allow-forms",
    "allow-scripts",
    "allow-downloads"
  
    ],
  "translations": [
    {
      "lang": "en",
      "parametersInformation": "Parameter Information text. Please enter the following parameters that are required to set up the extension."
    },
    {
      "lang": "de",
      "parametersInformation": "Parameter Informationstext. Bitte geben Sie die folgenden Parameter an, die für die Einrichtung der Erweiterung erforderlich sind."
    },
    {
      "lang": "fr",
      "parametersInformation": "Texte d'information sur les paramètres. Veuillez saisir les paramètres suivants qui sont nécessaires à la configuration de l'extension."
    }
  ],
  "parametersInformation": "Parameter Information text. Please enter the following parameters that are required to set up the extension.",
  "parameters": [
    {
      "name": "apiKey",
      "required": true,
      "type": "TEXT",
      "regexValidation": "^[a-zA-Z-]+$",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "API Key"
        },
        {
          "lang": "de",
          "value": "API-Schlüssel"
        }
      ],
      "placeholderTranslations": [
        {
          "lang": "en",
          "value": "API key received in the confirmation mail"
        },
        {
          "lang": "de",
          "value": "API-Schlüssel, den Sie in der Bestätigungsmail erhalten haben"
        }
      ]
    },
    {
      "name": "systemUrl",
      "required": true,
      "type": "TEXT",
      "regexValidation": null,
      "labelTranslations": [
        {
          "lang": "en",
          "value": "System URL"
        }
      ]
    },
    {
      "name": "demoNewParameter",
      "required": true,
      "type": "TEXT",
      "regexValidation": null,
      "labelTranslations": [
	      {
          "lang": "en",
          "value": "Show new Parameter"
        },
        {
          "lang": "de",
          "value": "Neuer Parameter anzeigen"
        }
      ]
    },
    {
      "name": "enableLogging",
      "type": "BOOLEAN",
      "labelTranslations": [
        {
          "lang": "en",
          "value": "Enable logging"
        },
        {
          "lang": "de",
          "value": "Logging aktivieren"
        }
      ]
    }
  ]
}
